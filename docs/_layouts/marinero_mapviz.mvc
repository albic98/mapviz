capture_directory: "~"
fixed_frame: map
target_frame: base_link
fix_orientation: true
rotate_90: false
enable_antialiasing: true
show_displays: false
show_status_bar: true
show_capture_tools: true
window_width: 1342
window_height: 2123
view_scale: 0.420075297
offset_x: 71.1018372
offset_y: -35.4210548
use_latest_transforms: true
background: "#a0a0a4"
image_transport: raw
displays:
  - type: mapviz_plugins/tile_map
    name: new display
    config:
      visible: true
      collapsed: true
      custom_sources:
        - base_url: http://localhost:8080/wmts/gm_layer/gm_grid/{level}/{x}/{y}.png
          max_zoom: 19
          name: Google19
          type: wmts
      bing_api_key: ""
      source: Google19
  - type: mapviz_plugins/tf_frame
    name: new display
    config:
      visible: true
      collapsed: true
      frame: map
      color: "#00ff00"
      draw_style: points
      position_tolerance: 0
      buffer_size: 0
      static_arrow_sizes: false
      arrow_size: 25
  - type: mapviz_plugins/tf_frame
    name: new display
    config:
      visible: true
      collapsed: false
      frame: base_link
      color: "#e01b24"
      draw_style: points
      position_tolerance: 0
      buffer_size: 0
      static_arrow_sizes: false
      arrow_size: 25
  - type: mapviz_plugins/navsat
    name: new display
    config:
      visible: true
      collapsed: false
      topic: /marinero/gps_sensor
      color: "#f6d32d"
      draw_style: lines
      position_tolerance: 0
      buffer_size: 0
      qos_depth: 10
      qos_history: 1
      qos_reliability: 1
      qos_durability: 2